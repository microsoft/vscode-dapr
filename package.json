{
	"name": "vscode-dapr",
	"displayName": "Dapr",
	"description": "A VS Code extension for users of Dapr.",
	"version": "0.0.2-alpha",
	"preview": true,
	"publisher": "ms-azuretools",
	"license": "SEE LICENSE IN LICENSE.md",
	"aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
	"extensionKind": [
		"workspace",
		"ui"
	],
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-dapr.applications.invoke-get",
		"onCommand:vscode-dapr.applications.invoke-post",
		"onCommand:vscode-dapr.applications.publish-message",
		"onCommand:vscode-dapr.tasks.scaffoldDaprTasks",
		"onCommand:workbench.action.tasks.runTask",
		"onView:vscode-dapr.views.applications"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode-dapr"
	},
	"homepage": "https://github.com/Microsoft/vscode-dapr/blob/master/README.md",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-dapr.applications.invoke-get",
				"title": "%vscode-dapr.applications.invoke-get.title%",
				"category": "Dapr"
			},
			{
				"command": "vscode-dapr.applications.invoke-post",
				"title": "%vscode-dapr.applications.invoke-post.title%",
				"category": "Dapr"
			},
			{
				"command": "vscode-dapr.applications.publish-message",
				"title": "%vscode-dapr.applications.publish-message.title%",
				"category": "Dapr"
			},
			{
				"command": "vscode-dapr.tasks.scaffoldDaprTasks",
				"title": "%vscode-dapr.tasks.scaffoldDaprTasks.title%",
				"category": "Dapr"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "vscode-dapr.applications.invoke-get",
					"when": "view == vscode-dapr.views.applications && viewItem == application"
				},
				{
					"command": "vscode-dapr.applications.invoke-post",
					"when": "view == vscode-dapr.views.applications && viewItem == application"
				},
				{
					"command": "vscode-dapr.applications.publish-message",
					"when": "view == vscode-dapr.views.applications && viewItem == application"
				}
			]
		},
		"problemMatchers": [
			{
				"name": "dapr",
				"fileLocation": [
					"relative",
					"${workspaceFolder}"
				],
				"pattern": [
					{
						"regexp": ".",
						"file": 1,
						"location": 2,
						"message": 3
					}
				],
				"background": {
					"beginsPattern": "^.*starting Dapr Runtime",
					"endsPattern": "^.*waiting on port"
				}
			}
		],
		"taskDefinitions": [
			{
				"type": "dapr",
				"properties": {
					"appId": {
						"type": "string",
						"description": ""
					},
					"appPort": {
						"type": "integer",
						"description": "",
						"default": -1
					},
					"command": {
						"type": "array",
						"description": "",
						"additionalItems": {
							"type": "string"
						}
					},
					"cwd": {
						"type": "string",
						"description": ""
					},
					"config": {
						"type": "string",
						"description": ""
					},
					"enableProfiling": {
						"type": "boolean",
						"description": "",
						"default": false
					},
					"grpcPort": {
						"type": "integer",
						"description": "",
						"default": -1
					},
					"httpPort": {
						"type": "integer",
						"description": "",
						"default": -1
					},
					"image": {
						"type": "string",
						"description": ""
					},
					"logLevel": {
						"type": "string",
						"description": "",
						"enum": [
							"debug",
							"info",
							"warning",
							"error",
							"fatal",
							"panic"
						],
						"default": "info"
					},
					"maxConcurrency": {
						"type": "integer",
						"description": ""
					},
					"placmentHost": {
						"type": "string",
						"description": "",
						"default": "localhost"
					},
					"profilePort": {
						"type": "integer",
						"description": "",
						"default": -1
					},
					"protocol": {
						"type": "string",
						"description": "",
						"enum": [
							"http",
							"grpc"
						],
						"default": "http"
					},
					"redisHost": {
						"type": "string",
						"description": "",
						"default": "localhost"
					}
				},
				"required": [
					"command"
				]
			},
			{
				"type": "daprd",
				"properties": {
					"allowedOrigins": {
						"type": "string",
						"description": "",
						"default": "*"
					},
					"alsoLogToStdError": {
						"type": "boolean",
						"description": ""
					},
					"appId": {
						"type": "string",
						"description": ""
					},
					"appPort": {
						"type": "integer",
						"description": ""
					},
					"componentsPath": {
						"type": "string",
						"description": "",
						"default": "./components"
					},
					"config": {
						"type": "string",
						"description": ""
					},
					"controlPlaneAddress": {
						"type": "string",
						"description": ""
					},
					"enableProfiling": {
						"type": "boolean",
						"description": "",
						"default": false
					},
					"grpcPort": {
						"type": "integer",
						"description": "",
						"default": 50001
					},
					"httpPort": {
						"type": "integer",
						"description": "",
						"default": 3500
					},
					"logLevel": {
						"type": "string",
						"description": "",
						"enum": [
							"debug",
							"info",
							"warning",
							"error",
							"fatal",
							"panic"
						],
						"default": "info"
					},
					"logBacktraceAt": {
						"type": "integer",
						"description": ""
					},
					"logDir": {
						"type": "string",
						"description": ""
					},
					"maxConcurrency": {
						"type": "integer",
						"description": ""
					},
					"mode": {
						"type": "string",
						"description": "",
						"enum": [
							"standalone",
							"kubernetes"
						],
						"default": "standalone"
					},
					"placementAddress": {
						"type": "string",
						"description": ""
					},
					"profilePort": {
						"type": "integer",
						"description": "",
						"default": 7777
					},
					"protocol": {
						"type": "string",
						"description": "",
						"enum": [
							"grpc",
							"http"
						],
						"default": "http"
					},
					"stdErrThreshold": {
						"type": "string",
						"description": "",
						"enum": [
							"debug",
							"info",
							"warning",
							"error",
							"fatal",
							"panic"
						]
					},
					"vLogLevel": {
						"type": "string",
						"description": "",
						"enum": [
							"debug",
							"info",
							"warning",
							"error",
							"fatal",
							"panic"
						]
					},
					"vLogFilters": {
						"type": "string",
						"description": ""
					}
				}
			},
			{
				"type": "daprd-down",
				"properties": {
					"appId": {
						"type": "string",
						"description": ""
					}
				},
				"required": [
					"appId"
				]
			}
		],
		"views": {
			"dapr-explorer": [
				{
					"id": "vscode-dapr.views.applications",
					"name": "%vscode-dapr.views.applications.name%"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dapr-explorer",
					"title": "Dapr",
					"icon": "assets/images/dapr.svg"
				}
			]
		}
	},
	"scripts": {
		"clean": "gulp clean",
		"build": "gulp build",
		"lint": "gulp lint",
		"watch": "tsc -watch -p ./",
		"package": "gulp package",
		"ci-build": "gulp ci-build",
		"ci-package": "gulp ci-package",
		"test": "gulp test"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.1",
		"@types/glob": "^7.1.1",
		"@types/gulp": "^4.0.6",
		"@types/gulp-sourcemaps": "0.0.32",
		"@types/handlebars": "^4.1.0",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.24",
		"@types/ps-list": "^6.2.1",
		"@types/vscode": "^1.41.0",
		"@typescript-eslint/eslint-plugin": "^2.17.0",
		"@typescript-eslint/parser": "^2.17.0",
		"del": "^5.1.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.5",
		"gulp": "^4.0.2",
		"gulp-eslint": "^6.0.0",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^6.0.0-alpha.1",
		"mocha": "^6.2.2",
		"ts-node": "^8.6.2",
		"tslint": "^5.20.0",
		"typescript": "^3.7.4",
		"vsce": "^1.71.0",
		"vscode-nls-dev": "^3.3.1",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"fs-extra": "^8.1.0",
		"handlebars": "^4.7.2",
		"ps-list": "^6.3.0",
		"vscode-azureextensionui": "^0.29.3",
		"vscode-nls": "^4.1.1"
	}
}
